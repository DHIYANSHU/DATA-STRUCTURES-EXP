#include <stdio.h>
#include <stdbool.h>

bool searchRegNo(int regNos[], int size, int target) {
    for (int i = 0; i < size; i++) {
        if (regNos[i] == target) {
            return true;
        }
    }
    return false;
}

int main() {
    int regNos[] = {101, 105, 203, 301, 405};
    int size = sizeof(regNos) / sizeof(regNos[0]);
    int target = 203;

    if (searchRegNo(regNos, size, target)) {
        printf("Registration number %d found.\n", target);
    } else {
        printf("Registration number %d not found.\n", target);
    }
    return 0;
}
