#include <stdio.h>
#include <stdbool.h>

void findTwiceRepeated(int arr[], int size) {
    int counts[1000] = {0};
    for (int i = 0; i < size; i++) {
        counts[arr[i]]++;
    }
    printf("Elements repeated twice: ");
    for (int i = 0; i < 1000; i++) {
        if (counts[i] == 2) {
            printf("%d ", i);
        }
    }
    printf("\n");
}

int main() {
    int arr[] = {1, 2, 3, 4, 1, 2, 5, 6};
    int size = sizeof(arr) / sizeof(arr[0]);
    findTwiceRepeated(arr, size);
    return 0;
}
