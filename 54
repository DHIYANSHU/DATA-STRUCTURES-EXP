#include <stdio.h>

void countFrequent(int arr[], int size) {
    int maxCount = 0;
    int maxElement = -1;
    for (int i = 0; i < size; i++) {
        int count = 1;
        for (int j = i + 1; j < size; j++) {
            if (arr[i] == arr[j]) {
                count++;
            }
        }
        if (count > maxCount) {
            maxCount = count;
            maxElement = arr[i];
        }
    }
    printf("Most frequently repeated number is %d, with a count of %d\n", maxElement, maxCount);
}

int main() {
    int arr[] = {1, 2, 3, 2, 4, 2, 5, 3};
    int size = sizeof(arr) / sizeof(arr[0]);
    countFrequent(arr, size);
    return 0;
}
