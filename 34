#include <stdio.h>

int fibonacciSum(int n) {
    if (n <= 0) return 0;
    if (n == 1) return 1;

    int a = 0, b = 1;
    int sum = 1;
    for (int i = 2; i <= n; i++) {
        int next = a + b;
        sum += next;
        a = b;
        b = next;
    }
    return sum;
}

int main() {
    int n = 10;
    printf("Sum of first %d Fibonacci numbers is: %d\n", n, fibonacciSum(n));
    return 0;
}
